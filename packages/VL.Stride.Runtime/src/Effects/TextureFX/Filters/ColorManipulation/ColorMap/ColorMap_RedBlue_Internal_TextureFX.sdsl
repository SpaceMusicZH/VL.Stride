[Category("Filter")]
[Summary("")]
shader ColorMap_RedBlue_Internal_TextureFX : ColorMapBase
{
    float4 ColorMap(float4 col)
    {
        float2 f = col.rb - col.g;
        return Ramp(f);
    };
};