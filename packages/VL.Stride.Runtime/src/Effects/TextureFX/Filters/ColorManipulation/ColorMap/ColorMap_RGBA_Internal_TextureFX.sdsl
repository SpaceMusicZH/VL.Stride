shader ColorMap_RGBA_Internal_TextureFX : ColorMapBase
{
    float4 ColorMap(float4 col)
    {
        col.r = Ramp(col.r).r;
        col.g = Ramp(col.g).g;
        col.b = Ramp(col.b).b;
        col.a = Ramp(col.a).a;
        return col;
    };
};