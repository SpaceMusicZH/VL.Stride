[Category("Filter")]
[Summary("")]
shader ColorConvert_HSV2RGB_Internal_TextureFX : ColorConvertBase
{
    float4 Convert(float4 col)
    {
        float3 h = HSVtoRGB(col.rgb);

        return float4(h.xyz, 1);
    }
};