[Category("Filter")]
[Summary("")]
shader Tunnels_Square_Internal_TextureFX : TunnelsBase
{
	stage float3 Tunnel(float2 p)
	{
		float2 pRot = r2dCyc(p, Rotation);
		float2 uv;
		float r = pow(pow(pRot.x*pRot.x,3) + pow(pRot.y*pRot.y,3), 1.0/(Distance*8));
		uv.x = .5*Time + 0.5/r;
		uv.y = (atan2(pRot.y,pRot.x)/3.1416+Offset);
		return float3(uv, r);
	}
};