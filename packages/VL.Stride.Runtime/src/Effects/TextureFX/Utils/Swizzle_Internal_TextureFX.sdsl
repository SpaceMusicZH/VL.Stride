[Category("Utils")]
[Summary("Rearranges the color channels")]
[Tags("convert")]
shader Swizzle_Internal_TextureFX : TextureFX
{
    int  Channel0;
    int  Channel1;
    int  Channel2;
    int  Channel3;
  
    stage override float4 Shading()
    {        
        float4 col = InTex();
    
        float4 newCol = float4 (col[Channel1], col[Channel2], col[Channel3], col[Channel0]);
    
        return newCol;
    }
};